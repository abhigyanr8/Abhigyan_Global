<?xml version="1.0" encoding="UTF-8"?>

<beans

  xmlns="http://www.springframework.org/schema/beans"

  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

  xmlns:p="http://www.springframework.org/schema/p"

  xsi:schemaLocation="http://www.springframework.org/schema/beans

        http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

<bean id="a" class="com.Friday_05.Account">   
<property name="balance" value="10000"></property>  
</bean>  
  
<bean id="e" class="com.Friday_05.Customer">  
<property name="first_name" value="Abhigyan"></property>  
<property name="Last_name" value="Ranjan"></property>  
<property name="account" ref="a"></property>  
</bean>  

</beans>

